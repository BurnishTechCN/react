{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "indent": [2, 2, {"SwitchCase": 1}],
        "arrow-body-style": 0,
        "eol-last": 0,
        "max-len": [2, 150, 2, {"ignoreComments": true, "ignoreUrls": true}],
        "camelcase": 0,
        "arrow-parens": 0,
        "react/jsx-no-bind": 0,
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": [0],
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "import/no-extraneous-dependencies": 0
    },
    "env": {
        "browser": true
    }
}
